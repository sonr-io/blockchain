{
    "go.languageServerFlags": [
        "-rpc.trace",
        "serve",
        "--debug=localhost:6060"
    ],
    "go.useLanguageServer": true,
    "material-icon-theme.opacity": 0.75,
    "material-icon-theme.saturation": 0.75,
    "material-icon-theme.files.associations": {
        "*.proto": "xml"
    },
    "files.exclude": {
        "**/package-lock.json": true,
        "**/**.pb.go": true,
        "go.sum": true,
        "buf.work": true,
        "LICENSE.md": true,
        ".sonr": true,
        "**/v1": true,
        "**/.classpath": true,
        "**/.factorypath": true,
        "**/.flutter-plugins": true,
        "**/.flutter-plugins-dependencies": true,
        "**/.packages": true,
        "**/.project": true,
        "**/.settings": true,
        "**/go.sum": false,
        "**/pubspec.lock": true,
        "**pubspec.lock": true,
        "**/node_modules": true,
        "app.iml": true,
        "types": true,
        "Contents.json": true,
        ".devcontainer": true,
        "docs": true,
        "highway": true,
        "motor": true,
        "vue": true,
        "**/design": true,
        "**/sonr.wiki": true,
        ".gitignore": true,
    },
    "material-icon-theme.folders.associations": {
        "sonrd": "interface",
        "rpc": "dist",
        "internal": "core",
        "x": "dist",
        "cmd": "dist",
        "proto": "json",
        "common": "lib",
        "testutil": "test",
        "did": "src",
        "ISSUE_TEMPLATE": "config",
        "workflows": "serverless",
        "electron": "examples",
        "flutter": "examples",
        "highway": "lib",
        "motor": "lib",
        "bin": "lib",
    },
    "github-actions.workflows.pinned.workflows": [
        ".github/workflows/notify.yml",
        ".github/workflows/release.yml",
        ".github/workflows/test.yml"
    ],
    "go.inferGopath": false
}
